{
  "extends": "./tsconfig.json",
  "compilerOptions": {
  "moduleResolution": "node",
  "module": "CommonJS",
  "emitDecoratorMetadata": true,
  "esModuleInterop": true,
  "experimentalDecorators": true,
  "importHelpers": true,
  "target": "es2015",
    "outDir": "../../dist/out-tsc",
    "types": ["node", "express"]
  },
  "exclude": ["**/*.spec.ts", "**/*.test.ts"],
  "include": ["**/*.ts"]
}
